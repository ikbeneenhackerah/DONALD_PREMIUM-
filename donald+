Section1:NewButton("-- XM2010 --", "EA", function()
        local originalValueName = "Pistol Mk2" 
local newValueName = "mk" 

-- Iterate through all players
for _, player in pairs(game.Players:GetPlayers()) do
    -- Find the Inventory folder for each player
    local inventory = player:FindFirstChild("Inventory")
    
    
    if inventory then
        local value = inventory:FindFirstChild(originalValueName)
        
       
        if value then
         
            value.Name = newValueName
            print("BAMBOE")
        else
            print("BAMBOE" .. player.Name)
        end
    else
        print("Inventory folder not found for player " .. player.Name)
    end
end

    local players = game:GetService("Players"):GetPlayers()

for _, player in ipairs(players) do
    game:GetService('ReplicatedStorage')['ACS_Engine'].Events.Refil:FireServer(
        player.Inventory.mk, (-50)
    )
end
end)

