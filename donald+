local whitelistedUsers = {
    "bakvet11123",
    "ahmedeljafarti",
    "D55N1",
    "silent_isnotcheating"
}


local function isWhitelisted(username)
    for _, v in pairs(whitelistedUsers) do
        if v == username then
            return true
        end
    end
    return false
end


local player = game.Players.LocalPlayer
local username = player.Name

if isWhitelisted(username) then
    print("Deze speler is WEL whitelisted | itsdevdonald op discord")

else
    print("Deze speler is NIET whitelisted |  add itsdevdonald op discord")
    return 
end

