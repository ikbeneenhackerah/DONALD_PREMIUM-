local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("DONALD PREMIUM+", "GrapeTheme")

local Tab1 = Window:NewTab("EA [ FIVER ]")
local Section1 = Tab1:NewSection("Emergency Amsterdam [ PRIVATE SERVER ONLY ]")

Section1:NewKeybind("SET OPEN/CLOSE KEYBIND", "Minimalize", Enum.KeyCode.Insert, function()
    Library:ToggleUI()
end)

Section1:NewButton("-- Politie XP --", "EA", function()
    local players = game:GetService("Players"):GetPlayers()

for _, player in ipairs(players) do
    game:GetService('ReplicatedStorage')['ACS_Engine'].Events.Refil:FireServer(
        player.Ranks.Police, (-1000)
    )
end
end)

