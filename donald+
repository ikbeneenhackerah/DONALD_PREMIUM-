local Section1 = Tab1:NewSection("-- Server Sounds --")

Section1:NewButton("-- Sounds Spammer --", "EA", function()
while task.wait() do

  for _, player in next, game.Players:GetPlayers() do
      game:GetService('ReplicatedStorage')['ACS_Engine'].Events.Whizz:FireServer(player)
  end
end
end)

local Section1 = Tab1:NewSection("-- Funda Nep --")

Section1:NewButton("-- Appartement --", "EA", function()
    local players = game:GetService("Players"):GetPlayers()

for _, player in ipairs(players) do
    local info = player:FindFirstChild("Information")
    if info then
        local appartementen = info:FindFirstChild("Appartementen")
        if appartementen then
            local appartement1 = appartementen:FindFirstChild("Appartement1")
            if appartement1 and appartement1:IsA("BoolValue") then
                appartement1.Value = true
            end
        end
    end
end
end)

Section1:NewButton("-- Money Factory --", "EA", function()
    local players = game:GetService("Players"):GetPlayers()

for _, player in ipairs(players) do
    local info = player:FindFirstChild("Information")
    if info then
        local appartementen = info:FindFirstChild("Appartementen")
        if appartementen then
            local moneyFactory = appartementen:FindFirstChild("MoneyFactory")
            if moneyFactory and moneyFactory:IsA("BoolValue") then
                moneyFactory.Value = true
            end
        end
    end
end
end)

Section1:NewButton("-- Witwas --", "EA", function()
    local players = game:GetService("Players"):GetPlayers()

for _, player in ipairs(players) do
    local info = player:FindFirstChild("Information")
    if info then
        local appartementen = info:FindFirstChild("Appartementen")
        if appartementen then
            local witwas = appartementen:FindFirstChild("Witwas")
            if witwas and witwas:IsA("BoolValue") then
                witwas.Value = true
            end
        end
    end
end
end)

